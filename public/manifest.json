{
    "id": "saudi-weather-app",
    "name": "Saudi Weather",
    "short_name": "Saudi Weather",
    "description": "A fast weather application providing real-time forecasts and weather updates.",
    "lang": "en",
    "dir": "ltr",
    "start_url": "/",
    "scope": "/",
    "display": "standalone",
    "display_override": [
        "tabbed",
        "window-controls-overlay",
        "standalone",
        "browser"
    ],
    "background_color": "#4facfe",
    "theme_color": "#4facfe",
    "orientation": "portrait-primary",
    "scope_extensions": [
        {
            "origin": "https://saudi-weather.syslink.dev"
        }
    ],
    "launch_handler": {
        "client_mode": "navigate-existing"
    },
    "icons": [
        {
            "src": "/icons/icon-72x72.png",
            "sizes": "72x72",
            "type": "image/png",
            "purpose": "maskable any"
        },
        {
            "src": "/icons/icon-96x96.png",
            "sizes": "96x96",
            "type": "image/png",
            "purpose": "maskable any"
        },
        {
            "src": "/icons/icon-128x128.png",
            "sizes": "128x128",
            "type": "image/png",
            "purpose": "maskable any"
        },
        {
            "src": "/icons/icon-144x144.png",
            "sizes": "144x144",
            "type": "image/png",
            "purpose": "maskable any"
        },
        {
            "src": "/icons/icon-152x152.png",
            "sizes": "152x152",
            "type": "image/png",
            "purpose": "maskable any"
        },
        {
            "src": "/icons/icon-192x192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "maskable any"
        },
        {
            "src": "/icons/icon-384x384.png",
            "sizes": "384x384",
            "type": "image/png",
            "purpose": "maskable any"
        },
        {
            "src": "/icons/icon-512x512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "maskable any"
        }
    ],
    "categories": [
        "weather",
        "utilities"
    ],
    "screenshots": [
        {
            "src": "/screenshots/screenshot-wide.png",
            "sizes": "1280x720",
            "type": "image/png",
            "form_factor": "wide",
            "label": "Saudi Weather App - Desktop View"
        },
        {
            "src": "/screenshots/screenshot-mobile.png",
            "sizes": "750x1334",
            "type": "image/png",
            "form_factor": "narrow",
            "label": "Saudi Weather App - Mobile View"
        }
    ],
    "shortcuts": [
        {
            "name": "Current Location Weather",
            "short_name": "My Location",
            "description": "Get weather for your current location",
            "url": "/",
            "icons": [
                {
                    "src": "/icons/icon-192x192.png",
                    "sizes": "192x192"
                }
            ]
        }
    ],
    "handle_links": "preferred",
    "prefer_related_applications": false,
    "related_applications": [
        {
            "platform": "play",
            "url": "https://play.google.com/store/apps/details?id=dev.syslink.rainy.twa",
            "id": "dev.syslink.rainy.twa"
        }
    ],
    "file_handlers": [
        {
            "action": "/open-file",
            "accept": {
                "application/json": [
                    ".json"
                ]
            },
            "name": "Open Weather JSON"
        }
    ],
    "protocol_handlers": [
        {
            "protocol": "web+saudiweather",
            "url": "/?weather=%s"
        }
    ],
    "share_target": {
        "action": "/share",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url"
        }
    },
    "iarc_rating_id": "eC1a2b3c4d5e6f7g8h9i0",
    "widgets": [
        {
            "name": "Current Weather",
            "description": "Show current weather on your desktop",
            "url": "/widget/current-weather"
        }
    ],
    "edge_side_panel": {
        "preferred_width": 400,
        "title": "Saudi Weather Side Panel",
        "icon": "/icons/icon-192x192.png"
    },
    "note_taking": {
        "new_note_url": "/notes/new"
    }
}